extends base

block meta
  - var title = 'Подкатегория каталога (автотовары)';

block wrapper
    .wrapper
      nav.pathway
        a.pathway__link(href="#") Главная
        a.pathway__link(href="#") Автотовары
        a.pathway__current(href="#") Авточехлы
      section.content
        h1 Авточехлы
      .category-counter.mb-4 1 840 товаров
      .row
        .col-xl-2.col-lg-3.mb-lg-4
          include base/filter-wheels.pug
        .col-xl-10.col-lg-9.mb-4

      include category/popular.pug

      .mb-3
        .h2.font-weight-bold.mb-4 Рекомендуем так же:
        include category/wheels.pug

      include base/links.pug

      .content.content_expanded.mb-5
        include base/content.pug
        .content__footer
          button.readmore.readmore_btn.readmore_arrow-down(type="button")
