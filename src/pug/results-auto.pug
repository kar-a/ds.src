                  option 30
            .col-4.col-lg-12
              .form__select.mb-lg-4
                .form__select-title Высота
                select.selecter
                  option --
                  option 9.5
                  option 10
                  option 10.5
                  option 11.5
                  option 12.5
                  option 13.5
            .col-4.col-lg-12
              .form__select
