extends results

block meta
  - var title = 'Результаты подбора по авто';
  - var h1 = 'Купить шины для HYUNDAI Accent 2000-2005 LC Sedan 1.3 в Ростове-на-Дону';

block filter
  .header__footer
    button.aside-toggler.flex-grow-1.aside-toggler_btn_group.px-3.py-2(type="button" data-target="#settings")
      +icon('car','aside-toggler__icon h3 mr-2')
      span.aside-toggler__title.h4.text-nowrap Подбор по авто
    button.aside-toggler.aside-toggler_btn_group.px-3.py-2(type="button" data-target="#params")
      +icon('compare','aside-toggler__icon h3')
      span.aside-toggler__title.aside-toggler__title_invisible.h4.text-nowrap Параметры подбора
    button.aside-toggler.aside-toggler_btn_group.px-3.py-2(type="button" data-target="#filter")
      +icon('filter','aside-toggler__icon h3')
      span.aside-toggler__title.aside-toggler__title_invisible.h4.text-nowrap
        span.aside-toggler__show Показать&nbsp;
        span.aside-toggler__hide Скрыть&nbsp;
        | фильтр

block results__sidebar
  #settings.aside
    .aside__wrapper
      form.form.form_bg.form_filter.mb-lg-3(action="#")
        .form__header.d-flex.align-items-center.justify-content-between.d-lg-block
          .form__title.form__title_icon.d-flex.align-items-center.flex-grow-1.h4.font-weight-bold.color-theme-brand.mb-0.mb-lg-2
            +icon('car','form__title-icon h3 mr-1')
            | Подбор по авто
          button.button-unstyled.h6(type="reset") Сбросить
        .form__section.border-0.mb-3
          .form__section-title.h6.color-theme-disabled.mb-4 Автомобиль
          .row
            .col-sm-6.col-md-3.col-lg-12
              .form__select.mb-4
                .form__select-title Марка
                select.selecter
                  option --
                  option ACURA
                  option ALFA ROMEO
                  option ASTON MARTIN
                  option AUDI
                  option BENTLEY
                  option BMW
            .col-sm-6.col-md-3.col-lg-12
              .form__select.mb-4
                .form__select-title Модель
                select.selecter
                  option --
                  option Azure
                  option Bentayga
                  option Brooklands
                  option Continental Flying Spur
                  option Continental GT
            .col-sm-6.col-md-3.col-lg-12
              .form__select.mb-4
                .form__select-title Кузов
                select.selecter
                  option --
                  option 2000-2005 LC
                  option 2006-2011 LC
                  option 2012-2017 LC
                  option 2018-2023 LC
                  option 2024-2028 LC
            .col-sm-6.col-md-3.col-lg-12
              .form__select
                .form__select-title Двигатель
                select.selecter
                  option --
                  option 1.3
                  option 1.4
                  option 1.5
                  option 1.6
        .form__section.border-0.pb-0
          .car-image.text-center.mb-5
            .image-wrapp.mb-4
              +img('content/results/white.jpg')
            .buttons-wrapp.but-row
              .car-color-wrapp.d-flex.flex-wrap.justify-content-center
                .car-color.color-white.mr-1.mb-2(data-color="white")
                .car-color.color-black.mr-1.mb-2(data-color="black")
                .car-color.color-red.mr-1.mb-2(data-color="red")
                .car-color.color-green.mr-1.mb-2(data-color="green")
                .car-color.color-blue.mr-1.mb-2(data-color="blue")
                .car-color.color-grey.mr-1.mb-2(data-color="grey")
                .car-color.color-bez.active.mr-1.mb-2(data-color="bez")
          a.d-flex.align-items-center.h5.text-nowrap.mb-4(href="#")
            +icon('tires','contacts__icon h2 color-theme-brand mr-3')
            | Подборать шины
          a.d-flex.align-items-center.h5.text-nowrap(href="#")
            +icon('wheels','contacts__icon h2 color-theme-brand mr-3')
            | Подборать диски

  #params.aside
    .aside__wrapper
      form.form.form_bg.form_filter.mb-lg-3(action="#")
        .form__header.d-flex.align-items-center.justify-content-between.d-lg-block
          .form__title.form__title_icon.d-flex.align-items-center.flex-grow-1.h4.font-weight-bold.color-theme-brand.mb-0.mb-lg-2
            +icon('compare','form__title-icon h3 mr-1')
            | Параметры подбора
        .form__section
          .form__section-title.h6.color-theme-disabled.mb-3 Рекомендованы производителем
          a.d-block.h6.mb-1(href="#") 175/70R13 (84)
          a.d-block.h6.mb-1(href="#") 185/60R14 (100)
          a.d-block.h6.mb-1(href="#") 185/55R15 (57)
        .form__section
          .form__section-title.h6.color-theme-disabled.mb-3 Взаимозаменяемые
          a.d-block.h6.mb-1(href="#") 205/60R13 (1)
          a.d-block.h6.mb-1(href="#") 175/65R14 (128)
          a.d-block.h6.mb-1(href="#") 175/55R15 (11)
          a.d-block.h6.mb-1(href="#") 195/50R15 (49)
          a.d-block.h6.mb-1(href="#") 205/50R15 (5)
          a.d-block.h6.mb-1(href="#")  215/45R15 (1)
        .form__section.border-0.pb-0
          .form__section-title.h6.color-theme-disabled.mb-3 Тюнинг
          a.d-block.h6.mb-1(href="#") 185/50R16 (12)
          a.d-block.h6.mb-1(href="#") 195/45R16 (22)
          a.d-block.h6.mb-1(href="#") 205/40R16 (2)
          a.d-block.h6.mb-1(href="#") 205/45R16 (20)
          a.d-block.h6.mb-1(href="#") 215/40R16 (4)
          a.d-block.h6.mb-1(href="#") 195/40R17 (3)

