                #tires.panel__grid.panel__grid_active.col-xl-10
                  .panel__content
                    .h4.mb-4.d-none.d-xl-block Производители автошин:
                    button.panel__toggler(type="button")
                      +icon('arrow-left','h6 color-theme-brand pl-3 mr-2')
                      span.h5 Назад
                    .panel__subcategory.mb-4.mb-xl-0
                      each logolist__item in logolist
                        a.brand.brand_border_bottom.d-flex.align-items-center.justify-content-around.d-xl-inline-block.px-3.py-2.p-xl-0.mr-xl-3.mb-xl-4(href="#")
                          span.d-inline-block.pl-3.pr-1.px-xl-0
                            +img(logolist__item.img)
                          span.h5.text-nowrap.d-block.d-xl-none!= logolist__item.title
                    .text-center.d-none.d-xl-block
                      a.readmore(href="#")
                        span.mr-2.pointer-events-none Показать еще
                        +icon('readmore','readmore__icon h3 color-theme-brand')
                  .panel__aside
                    .mb-4
                      a.toggler.py-1(href="#") Зимние шины
                      a.toggler.py-1(href="#") Летние шины
                      a.toggler.py-1(href="#") Шины для внедорожников
                      a.toggler.py-1(href="#") Всесезонные шины
                      a.toggler.py-1(href="#") Шины на Газель
                    .banner.banner_sm.overflow-hidden.mb-4.mb-xl-0
                      +img('content/banners/banner-2.jpg','img-cover')
                      .banner__inner.d-flex.flex-column.align-items-center.justify-content-between.p-4
                        .banner__title.h2.font-weight-bold.color-theme-white Подбор запчастей по VIN-коду
                        a.button.text-uppercase(href="#") Подобрать
                #moto.panel__grid.col-xl-10
                  .panel__content
                    .h4.mb-4.d-none.d-xl-block Производители мотошин:
                    button.panel__toggler(type="button")
                      +icon('arrow-left','h6 color-theme-brand pl-3 mr-2')
                      span.h5 Назад
                    .panel__subcategory.mb-4.mb-xl-0
                      each logolist__item in logolist
                        a.brand.brand_border_bottom.d-flex.align-items-center.justify-content-around.d-xl-inline-block.px-3.py-2.p-xl-0.mr-xl-3.mb-xl-4(href="#")
                          span.d-inline-block.pl-3.pr-1.px-xl-0
                            +img(logolist__item.img)
                          span.h5.text-nowrap.d-block.d-xl-none!= logolist__item.title
                    .text-center.d-none.d-xl-block
                      a.readmore(href="#")
                        span.mr-2.pointer-events-none Показать еще
                        +icon('readmore','readmore__icon h3 color-theme-brand')
                  .panel__aside
                    .mb-4
                      a.toggler.py-1(href="#") Летние шины
                      a.toggler.py-1(href="#") Шины для внедорожников
                      a.toggler.py-1(href="#") Зимние шины
                      a.toggler.py-1(href="#") Всесезонные шины
                    .banner.banner_sm.overflow-hidden.mb-4.mb-xl-0
                      +img('content/banners/banner-2.jpg','img-cover')
                      .banner__inner.d-flex.flex-column.align-items-center.justify-content-between.p-4
                        .banner__title.h2.font-weight-bold.color-theme-white Подбор запчастей по VIN-коду
                        a.button.text-uppercase(href="#") Подобрать
                #wheels.panel__grid.col-xl-10
                  .panel__content
                    .h4.mb-4.d-none.d-xl-block Производители дисков
                    button.panel__toggler(type="button")
                      +icon('arrow-left','h6 color-theme-brand pl-3 mr-2')
                      span.h5 Назад
                #battery.panel__grid.col-xl-10
                  .panel__content
                    .h4.mb-4.d-none.d-xl-block Производители аккумуляторов
                    button.panel__toggler(type="button")
                      +icon('arrow-left','h6 color-theme-brand pl-3 mr-2')
                      span.h5 Назад
                #detail.panel__grid.col-xl-10
                  .panel__content
                    .h4.mb-4.d-none.d-xl-block Производители автотоваров
                    button.panel__toggler(type="button")
                      +icon('arrow-left','h6 color-theme-brand pl-3 mr-2')
                      span.h5 Назад
                #oil.panel__grid.col-xl-10
                  .panel__content
                    .h4.mb-4.d-none.d-xl-block Производители масел
                    button.panel__toggler(type="button")
                      +icon('arrow-left','h6 color-theme-brand pl-3 mr-2')
                      span.h5 Назад
    .header__col.header__col_side_right.col-xl-8.col-lg-7
      .search-panel.search-panel_active
        .search-panel__wrapper
          button.panel-close-btn(type="button")
            +icon('close','h3 mr-2')
            span.h4.text-uppercase Поиск
          form.search(action="#")
            input.search__field(type="text" value="" placeholder="Я ищу...")
            button.search__btn.search__btn_type_reset(type="reset")
              +icon('close-noborder')
            button.search__btn.search__btn_type_submit(type="submit")
              +icon('search')
      .header__bottom-panel
        block profile
          .header__navbar
            button.header__navbar-item.header__navbar-item_toggler_search(type="button")
              span.header__navbar-icon
                +icon('search')
              span.header__navbar-caption Поиск
            a.header__navbar-item(href="#")
              span.header__navbar-icon
                +icon('compare')
              span.header__navbar-caption Сравнить
            a.header__navbar-item(href="#")
              span.header__navbar-icon
                +icon('car')
              span.header__navbar-caption Выбор авто
            a.header__navbar-item(href="#")
              span.header__navbar-icon
                +icon('case')
              span.header__navbar-caption Заказы
            button.header__navbar-item(data-toggle="modal" data-target="#auth-phone")
              span.header__navbar-icon
                +icon('user')
              span.header__navbar-caption Вход | регистрация
          .header__aside
            a.header__btn.header__btn_place_panel(href="#")
              span.header__btn-icon.header__btn-icon_type_holder
                +icon('cart')
              span.header__btn-title Корзина
