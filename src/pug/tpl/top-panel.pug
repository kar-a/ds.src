header.header
  .header__wrapper.wrapper
    .header__col.header__col_side_left.col-xl-4.col-lg-5.px-0.px-lg-1
      a.logo.logo_place_header.color-theme-brand.mr-4(href="#")
        +svg('logo')
      .header__togglers
        block btns
          button.header__btn.header__btn_type_toggler.text-uppercase.mr-3(type="button" data-target="#categories")
            +icon('menu','header__btn-icon header__btn-icon_show')
            +icon('close','header__btn-icon header__btn-icon_close')
            span.header__btn-title Каталог
          button.header__btn.header__btn_type_toggler.text-uppercase(type="button" data-target="#services")
            +icon('services','header__btn-icon header__btn-icon_show')
            +icon('close','header__btn-icon header__btn-icon_close')
            span.header__btn-title Услуги
        .panel
          #services.panel__wrapper
            button.panel-close-btn(type="button")
              +icon('close','h3 mr-2')
              span.h4.text-uppercase услуги
            .wrapper
              .row
                .panel__nav.border-0.col-xl-4
                  a.panel__nav-link.panel__nav-link_type_plain(href="#")
                    +icon('services-tires','h3 color-theme-brand mr-2')
                    span.h4 Шиномонтаж
                  a.panel__nav-link.panel__nav-link_type_plain(href="#")
                    +icon('services-store','h3 color-theme-brand mr-2')
                    span.h4 Сезонное хранение шин и дисков
                  a.panel__nav-link.panel__nav-link_type_plain(href="#")
                    +icon('services-aircond','h3 color-theme-brand mr-2')
                    span.h4 Заправка автокондиционеров
                .panel__nav.col-xl-4.border-0.mb-5.mb-xl-0
                  a.panel__nav-link.panel__nav-link_type_plain(href="#")
                    +icon('services-wheel-rolled','h3 color-theme-brand mr-2')
                    span.h4 Прокатка дисков
                  a.panel__nav-link.panel__nav-link_type_plain(href="#")
                    +icon('services-wheel-painted','h3 color-theme-brand mr-2')
                    span.h4 Покраска дисков
                  a.panel__nav-link.panel__nav-link_type_plain(href="#")
                    +icon('services-vin','h3 color-theme-brand mr-2')
                    span.h4 Подбор по VIN
                .col-xl-4
                  .banner.banner_xs.overflow-hidden.mb-4.mb-xl-0
                    +img('content/banners/banner-1.jpg','img-cover')
                    .banner__inner.d-flex.flex-column.align-items-start.justify-content-between.p-4
                      .banner__title.h2.font-weight-bold.color-theme-brand Запись на шиномонтаж онлайн
                      a.button.text-uppercase(href="#") Записаться
          #categories.panel__wrapper
            button.panel-close-btn(type="button")
              +icon('close','h3 mr-2')
              span.h4.text-uppercase каталог
            .wrapper
              .row.align-items-start
                .panel__nav.col-xl-2.pr-xl-2
                  .panel__nav-item(data-target="#tires")
                    a.panel__nav-link.panel__nav-link_type_btn.panel__nav-link_active(href="#")
                      +icon('tires','h3 color-theme-brand mr-2')
                      span.panel__nav-title Автошины
                    button.panel__nav-btn(type="button")
                      +icon('arrow-right','color-theme-brand')
                  .panel__nav-item(data-target="#moto")
                    a.panel__nav-link.panel__nav-link_type_btn(href="#")
                      +icon('moto','h3 color-theme-brand mr-2')
                      span.panel__nav-title Мотошины
                    button.panel__nav-btn(type="button")
                      +icon('arrow-right','color-theme-brand')
                  .panel__nav-item(data-target="#wheels")
                    a.panel__nav-link.panel__nav-link_type_btn(href="#")
                      +icon('wheels','h3 color-theme-brand mr-2')
                      span.panel__nav-title Диски
                    button.panel__nav-btn(type="button")
                      +icon('arrow-right','color-theme-brand')
                  .panel__nav-item(data-target="#battery")
                    a.panel__nav-link.panel__nav-link_type_btn(href="#")
                      +icon('battery','h3 color-theme-brand mr-2')
                      span.panel__nav-title Аккумуляторы
                    button.panel__nav-btn(type="button")
                      +icon('arrow-right','color-theme-brand')
                  .panel__nav-item(data-target="#detail")
                    a.panel__nav-link.panel__nav-link_type_btn(href="#")
                      +icon('detail','h3 color-theme-brand mr-2')
                      span.panel__nav-title Автотовары
                    button.panel__nav-btn(type="button")
                      +icon('arrow-right','color-theme-brand')
                  .panel__nav-item(data-target="#oil")
                    a.panel__nav-link.panel__nav-link_type_btn(href="#")
                      +icon('oil','h3 color-theme-brand mr-2')
                      span.panel__nav-title Масла
                    button.panel__nav-btn(type="button")
                      +icon('arrow-right','color-theme-brand')
